datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
}

model User {
  id            Int    @id @default(autoincrement())
  name          String?
  email         String?   @unique
  password      String?
}

model Plans{
  id Int @id @default(autoincrement())
  name String @unique
  price Float
  description String?
  duration Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Logs{
  id Int @id @default(autoincrement())
  message String
  createdAt String


}
